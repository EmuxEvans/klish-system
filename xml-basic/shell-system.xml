<?xml version="1.0" encoding="UTF-8"?>
<CLISH_MODULE xmlns="http://clish.sourceforge.net/XMLSchema"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://clish.sourceforge.net/XMLSchema
                     http://clish.sourceforge.net/XMLSchema/clish.xsd">

  <VIEW name="main">

    <COMMAND name="interact system"
			       help="Start system shell"
             interrupt="true">
      <ACTION>$SHELL</ACTION>
    </COMMAND>

    <COMMAND name="interact bash"
			       help="Start system bash"
             interrupt="true">
      <ACTION>bash</ACTION>
    </COMMAND>

    <COMMAND name="interact sh"
			       help="Start system sh"
             interrupt="true">
      <ACTION>sh</ACTION>
    </COMMAND>

  </VIEW>

</CLISH_MODULE>
