<?xml version="1.0" encoding="UTF-8"?>
<CLISH_MODULE xmlns="http://clish.sourceforge.net/XMLSchema"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://clish.sourceforge.net/XMLSchema
                     http://clish.sourceforge.net/XMLSchema/clish.xsd">

	<PTYPE name="rt_nud"
		     pattern="all permanent reachable noarp stale"
		     help="Neighhour state"/>
	<PTYPE name="rt_tos"
         pattern="[[:graph:]]+"
         help="Routing ToS"/>
	<PTYPE name="rt_protocol"
         pattern="[[:graph:]]+"
         help="Routing protocol"/>
	<PTYPE name="rt_realm"
         pattern="[[:graph:]]+"
         help="Routing realm"/>
	<PTYPE name="rt_scope"
         pattern="[[:graph:]]+"
         help="Routing scope"/>
	<PTYPE name="rt_table"
         pattern="[[:graph:]]+"
         help="Routing table"/>

</CLISH_MODULE>
